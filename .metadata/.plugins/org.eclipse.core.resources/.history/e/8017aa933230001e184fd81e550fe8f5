package algorithm;

import java.util.Scanner;

public class SWEA1208 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		for (int t = 1; t <= 10; t++) {
			int dump = sc.nextInt();
			
			int[] array = new int[100];
			int max = 0;
			int min = 100;
			for (int i = 0; i < 100; i++) {
				int num = sc.nextInt();
				array[i] = num;
				
				if(array[i] > max) {
					max = num;
				}
				if(array[i] < min) {
					min = num;
				}
			}
			
		}
	}

}
